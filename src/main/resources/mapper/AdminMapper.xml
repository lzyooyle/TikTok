<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dpb.mapper.AdminMapper">
    <insert id="insertAdmin" parameterType="Admin">
        insert into t_admin(username,password) values(#{username},#{password})
    </insert>
    <select id="queryByName" resultType="Admin">
        select username,password from t_admin where username=#{username};
    </select>
</mapper>
