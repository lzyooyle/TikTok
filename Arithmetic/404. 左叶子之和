   public int sumOfLeftLeaves(TreeNode root) {
        Queue<TreeNode> queue = new LinkedList<>();
        queue.offer(root);
        int total = 0;
        while(!queue.isEmpty()){
            TreeNode node = queue.poll();
            if(node.left!=null){
                queue.offer(node.left);
                if(node.left.left==null && node.left.right==null){
                    total+=node.left.val;
                }
            }
            if(node.right!=null){
                queue.offer(node.right);
            }
        }
        return total;
    }
}
