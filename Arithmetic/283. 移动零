class Solution {
    public void moveZeroes(int[] nums) {
        int fast = 0;
        int slow;
        for(slow = 0;fast<nums.length;fast++){
            if(nums[fast]!=0){
                nums[slow]=nums[fast];
                slow++;
            }
        }
        for(;slow < nums.length;slow++){
            nums[slow] = 0;
        }
        System.out.println(Arrays.toString(nums)+"::"+slow);
    }
}
