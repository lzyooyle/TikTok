class Solution {
    public int[] dailyTemperatures(int[] temperatures) {
        int[] dp=new int[temperatures.length];
        int len=temperatures.length;
        for(int i=0;i<len;i++){
            for(int j=i+1;j<len;j++){
                if(temperatures[i]<temperatures[j]){
                    dp[i]=j-i;
                    break;
                }
            }
        }
        return dp;
    }
}
class Solution {
    public int[] dailyTemperatures(int[] temperatures) {
        int[] dp=new int[temperatures.length];
        int len=temperatures.length;
        for(int start=0,i=1;i<len;i++){
            if(temperatures[i]>temperatures[start]){
               dp[start]=i-start;
               start++;
               i=start;
            }
            if(i==len-1 && i-start>0){
                start++;
                i=start;
            }
        }
        return dp;
    }
}
