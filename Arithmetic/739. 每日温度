class Solution {
    public int[] dailyTemperatures(int[] temperatures) {
        int[] dp=new int[temperatures.length];
        int len=temperatures.length;
        for(int i=0;i<len;i++){
            for(int j=i+1;j<len;j++){
                if(temperatures[i]<temperatures[j]){
                    dp[i]=j-i;
                    break;
                }
            }
        }
        return dp;
    }
}
