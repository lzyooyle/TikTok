class Solution {
    public int[] smallerNumbersThanCurrent(int[] nums) {
        int[] dp=new int[nums.length];        
        Map<Integer,Integer> map=new HashMap<>();
        for(int i=0,num=0;i<nums.length;i++){
            if(map.get(nums[i])!=null){
                dp[i]=map.get(nums[i]);
                continue;
            }
            for(int j=0;j<nums.length;j++){
                if(nums[j]<nums[i]){
                    num++;
                }
            }
            dp[i]=num;
            map.put(nums[i],num);
            num=0;
        }
        return dp;
    }
}
