class Solution {
    public int[] intersect(int[] nums1, int[] nums2) {
        ArrayList<Integer> set1 = new ArrayList();
        for(int i : nums1){
            set1.add(i);
        }
        ArrayList<Integer> set3 = new ArrayList();
        for(int i : nums2){
            if(set1.contains(i)){
                set1.remove(new Integer(i));
                set3.add(i);
            }
        }
        //System.out.println(set1+"::"+set2+"::"+set3);
        int[] arr3=new int[set3.size()];
        int num = 0;
        for (Integer o : set3){
            arr3[num++] = o;
        }
        return arr3;
    }
}
