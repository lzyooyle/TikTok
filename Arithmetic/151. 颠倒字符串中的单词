class Solution {
    public String reverseWords(String s) {
        s = s.trim();
        int len = s.length();
        StringBuilder str = new StringBuilder();
        List<StringBuilder> list = new ArrayList<>();
        for(int i = 0; i < len; i++){
            str.append(s.charAt(i));
            if(s.charAt(i)==' '){
                while(i > 0 && s.charAt(i)==s.charAt(i+1)){
                    i++;
                }
                list.add(str);
                str = new StringBuilder();
            }
        }
        list.add(str);  
        int num = list.size();      
        for(int i = 0; i < num; i++){
            int right = num-1;
            while(i <= right){
                StringBuilder temp = list.get(i);
                list.set(i,list.get(right));
                list.set(right,temp);
                right--;
                i++;
            }
            if(i > right){
                break;
            }
        }
        StringBuilder result = new StringBuilder();
        for(int i = 0; i < num; i++){
            result.append(list.get(i).toString().trim()+" ");
        }
        System.out.println(list);
        s = result.toString().trim();
        return s;
    }
}
class Solution {
    public String reverseWords(String s) {
        s = s.trim();
        int len = s.length();
        StringBuilder str = new StringBuilder();
        for(int i = 0; i < len; i++){
            if(s.charAt(i)==' '){
                while(i > 0 && s.charAt(i)==s.charAt(i+1)){
                    i++;
                }
                str.append("#");
            }else{

                str.append(s.charAt(i));
            }
        }
        System.out.println(str);
        String[] arr = str.toString().split("#");
        int num = arr.length;
        for(int left = 0; left < num; left++){
            int right = num-1;
            while(left<=right){
                String temp = arr[left];
                arr[left] = arr[right];
                arr[right] = temp;
                left++;
                right--;
            }
            if(left > right){
                break;
            }
        }
        String result = "";
        for(String s1 : arr){
            result+=s1+" ";
        }
        System.out.println(result);
        return result.trim();
    }
}
class Solution {
    public String reverseLeftWords(String s, int n) {
        StringBuilder str = new StringBuilder(s);
        int end = n-1;
        swap(str,0,end);
        swap(str,n,str.length()-1);
        return str.reverse().toString();
    }
    public void swap(StringBuilder str, int start, int end){
        while(start <= end){
            char c = str.charAt(start);
            str.setCharAt(start,str.charAt(end));
            str.setCharAt(end,c);
            start++;
            end--;
        }
    }
}
