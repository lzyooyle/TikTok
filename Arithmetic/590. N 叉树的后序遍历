class Solution {
    public List<Integer> postorder(Node root) {
        List<Integer> list = new ArrayList<>();
        if(root==null){
            return list;
        }
        Stack<Node> stack = new Stack<>();
        stack.push(root);
        while(!stack.isEmpty()){
            Node node = stack.pop();
            list.add(node.val);
            for(int i = 0; i <node.children.size(); i++){
                stack.push(node.children.get(i));
            }
        }
        Collections.reverse(list);
        return list;
    }
}
