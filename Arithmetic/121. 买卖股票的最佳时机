class Solution {
    public int maxProfit(int[] prices) {
        int min=prices[0],plus=0,max=0;
        for(int i=1;i<prices.length;i++){
            if(min>prices[i]){
                min=prices[i];
            }else{
                plus=prices[i]-min;
                if(max<plus){
                   max=plus; 
                }
            }
        }
        return max;
    }
}
