class Solution {
    public int pivotIndex(int[] nums) {
        int left=-1,right=0,len=nums.length,temp=0;
        for(int i=0;i<len;i++){
            left=count(nums,0,i);
            right=count(nums,i+1,len);
            if(left==right){
                temp=i;
                break;            
            }
        }
        return left==right?temp:-1;
    }
    private int count(int[] nums,int start,int end){
        int sum=0;
        for(int i=start;i<end;i++) sum+=nums[i];
        return sum;
    }
}
