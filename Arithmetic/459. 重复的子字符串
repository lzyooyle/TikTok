class Solution {
    public boolean repeatedSubstringPattern(String s) {
        int j = 0;
        int[] next = new int[s.length()];
        next[0]=0;
        for(int i = 1; i < s.length(); i++){
            while(j > 0 && s.charAt(j)!=s.charAt(i)){
                j = next[j-1];
            }
            if(s.charAt(j)==s.charAt(i)){
                j++;
            }
            next[i] = j;
        }
        int len = next.length;
        int left = len - next[len-1];
        System.out.println(Arrays.toString(next));        
        System.out.println(left);
        int max = Arrays.stream(next).max().getAsInt();
        if(max!=next[len-1]){
            return false;
        }        
        if(left == len){
            return false;
        }
        if(len%left == 0){
            return true;
        }else{
            return false;
        }
    }
}
