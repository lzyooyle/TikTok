class Solution {
    public int minDepth(TreeNode root) {
        if(root==null){
            return 0;
        }
        Queue<TreeNode> queue = new LinkedList<>();
        queue.offer(root);
        int num = 0;
        Boolean flag = false;
        while(!queue.isEmpty()){
            if(flag){
                break;
            }
            int size = queue.size();
            for(int i = 0; i < size; i++){
                TreeNode cur = queue.poll();
                if(i+1==size){

                }else{

                }
                if(cur.left!=null){
                    queue.offer(cur.left);
                }
                if(cur.right!=null){
                    queue.offer(cur.right);
                }
                if(cur.left == null && cur.right==null){
                    flag = true;
                    break;
                }
            }
            num++;
        }
        return num;
    }
}
