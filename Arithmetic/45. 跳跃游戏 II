class Solution {
    public int jump(int[] nums) {
        if(nums.length==1) return 0;
        int temp=0,max=0,result=0;
        List<Integer> list = new LinkedList<>();
        for(int i = 0,j=0; i <=temp; i++){
            j=nums[i]+i;
            if(max<j){
                max=j;
            }
            if(i==temp){
                temp=max;
                System.out.println(i+"::"+max);                                
                list.add(max);
            }
            if(max>=nums.length-1){
                break;
            }
        }
        if(list.get(list.size()-1)>=nums.length-1){
            result=list.size();
        }else{
            result=list.size()+1;
        }
        return result;
    }
}
