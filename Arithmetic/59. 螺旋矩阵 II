     59. 螺旋矩阵 II
     */
    public static int[][] rope(int n ) {
        int[][] nums = new int[n][n];
        int count=1;
        if(n==1){
            nums[0][0]=1;
            return nums;
        }
        for (int i = 0; i < n; i++) {
            for (int j = 0; j < n; j++) {
                nums[i][j]=count;
                count++;
                //right
                if(i>0 && j>=n/2 && i<=j){
                    if(n-j-1==0){
                        nums[i][j]=nums[n-j-1][j]+i;
                    }else{
                        nums[i][j]=nums[n-j-1][j]+i+j-n+1;
                    }
                }
                //bottom
                if(i>=n/2 && j<=i && j>=n-i-1){
                    nums[i][j]=nums[i-1][i]+1+i-j;
                }

                //top
                if(i> 0 && i<=n/2 && j<n-i && j>0){
                    nums[i][j]=nums[i-1][j-1]+(n-(i-1)*2-1)*4;
                }
                //left
                if(i>j && j<n/2 && i<n-j-1 && i!=j){
                    if(j>0){
                        nums[i][j]=nums[j][j]+(n-j*2-1)*4-i+j;
                    }else{
                        nums[i][j]=nums[j][j]+(n-j*2-1)*4-i;
                    }
                }
            }
        }
        return nums;
    }
